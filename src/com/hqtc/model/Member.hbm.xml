<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hqtc.model.Member" table="member" schema="" catalog="dessert">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="account">
            <column name="account" sql-type="varchar" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" not-null="true"/>
        </property>
        <property name="position">
            <column name="position" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="shopid">
            <column name="shopid" sql-type="int" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>