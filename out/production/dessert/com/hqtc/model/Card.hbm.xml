<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hqtc.model.Card" table="card" schema="" catalog="dessert">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="customerId">
            <column name="customerId" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="lastTme">
            <column name="lastTme" sql-type="date" length="10" not-null="true"/>
        </property>
        <property name="money">
            <column name="money" sql-type="float" length="12" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>