<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hqtc.model.Dessert" table="dessert" schema="" catalog="dessert">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="float" length="12" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>