<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hqtc.model.Order" table="order" schema="" catalog="dessert">
        <id name="id">
            <column name="id" sql-type="int" length="10" not-null="true"/>
        </id>
        <property name="shopid">
            <column name="shopid" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="salerid">
            <column name="salerid" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="customerid">
            <column name="customerid" sql-type="int" length="10" not-null="true"/>
        </property>
        <property name="money">
            <column name="money" sql-type="float" length="12" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>